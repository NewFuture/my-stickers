{
    "version": "2.0",
    "logging": {
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "maxTelemetryItemsPerSecond": 10,
                "initialSamplingPercentage": 60,
                "excludedTypes": "Dependency;Event,Request,PageView"
            },
            "enableLiveMetrics": true,
            "enableDependencyTracking": false,
            "enablePerformanceCountersCollection": false
        }
    }
}
